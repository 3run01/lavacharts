{
    "name": "khill/lavacharts",
    "version": "1.0.0_rc1",
    "license": "GPLv3",
    "description": "A PHP wrapper library for the Google Chart API",
    "authors": [
        {
            "name": "Kevin Hill",
            "email": "kevinkhill@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.0"
    },
    "require-dev": {
    	"phpunit/phpunit": "4.1.*",
        "mockery/mockery": "dev-master",
        "squizlabs/php_codesniffer": "1.*",
        "nette/nette": "~2.1.3",
        "apigen/apigen": "2.*"
    },
    "autoload": {
        "psr-0": { "Khill\\Lavacharts": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Khill\\Lavacharts\\Tests\\": "tests/" }
    },
    "minimum-stability": "dev"
}
