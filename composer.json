{
    "name": "khill/lavacharts",
    "description": "PHP wrapper library for the Google Chart API",
    "keywords": ["composer", "packagist", "google", "charts", "graphs"],
    "version": "2.0.0-beta1",
    "homepage": "http://kevinkhill.github.io/Lavacharts",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Kevin Hill",
            "email": "kevinkhill@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "nesbot/carbon": "1.10.*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.1.*",
        "mockery/mockery": "0.9.*",
        "squizlabs/php_codesniffer": "1.*",
        "sami/sami": "2.0.*",
        "satooshi/php-coveralls": "0.6.*"
    },
    "autoload": {
        "psr-4": { "Lavacharts\\": "src/Khill/Lavacharts/" }
    },
    "autoload-dev": {
        "psr-4": { "Lavacharts\\Tests\\": "tests/" }
    },
    "minimum-stability": "stable",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev",
            "dev-2.0": "2.0.x-dev"
        }
    }
}
